{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "x3p0/powered-by",
	"version": "1.0.0",
	"title": "X3P0 - Powered By",
	"category": "widgets",
	"icon": "comments",
	"description": "Displays a random \"powered by\" message.",
	"attributes": {
		"poweredByType": {
			"type": "string",
			"default": ""
		},
		"textAlign": {
			"type": "string"
		}
	},
	"supports": {
		"anchor": false,
		"align": true,
		"html": false,
		"__experimentalBorder": {
			"radius": true,
			"color": true,
			"width": true,
			"style": true,
			"__experimentalDefaultControls": {
				"width": true,
				"color": true
			}
		},
		"color": {
			"gradients": true,
			"__experimentalDefaultControls": {
				"background": true,
				"text": true
			}
		},
		"spacing": {
			"margin": true,
			"padding": true,
			"__experimentalDefaultControls": {
				"padding": true
			}
		},
		"typography": {
			"fontSize": true,
			"lineHeight": true,
			"__experimentalFontStyle": true,
			"__experimentalFontWeight": true,
			"__experimentalFontFamily": true,
			"__experimentalTextTransform": true,
			"__experimentalDefaultControls": {
				"fontSize": true
			}
		}
	},
	"textdomain": "x3p0-powered-by",
	"editorScript": "file:./index.js",
	"style": "file:./style-index.css"
}
